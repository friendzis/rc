{
    "install":{
        "editors/vim/.vimrc":{
            "type":"file",
            "method":"link",
            "destination":"~/.vimrc"
        },
        "editors/vim/.vim":{
            "type":"directory",
            "method":"copy",
            "destination":"~/.vim/",
            "options":[
                "--no-backup"
            ]
        },
        "scm/git/.gitconfig":{
            "type":"file",
            "method":"copy",
            "destination":"~/.gitconfig"
        }
    }
}
